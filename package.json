{
  "name": "Teikei",
  "version": "0.0.7",
  "description": "An interactive map for community supported agriculture.",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "lint": "node_modules/eslint/bin/eslint.js client",
    "banner": "node_modules/asciify/bin/asciify.js -f ivrit Teikei",
    "start:client": "node client/index.js --config client/nwb.config.js",
    "start:server": "bundle exec guard",
    "start:dev": "npm run kill -s && npm run banner -s && npm run start:client -s & npm run start:server -s",
    "start:prod": "RAILS_ENV=production bundle exec rails server thin",
    "start": "npm run start:dev -s",
    "build:client": "nwb build --config client/nwb.config.js client/src/index.js",
    "build:server": "bundle exec rake assets:precompile",
    "build": "npm run -s build:client && npm run -s build:server",
    "kill": "killall node & killall ruby ; echo 'killed all running processes'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teikei/teikei"
  },
  "keywords": [
    "csa",
    "farming",
    "agriculture"
  ],
  "author": "Simon Jockers, Daniel Mack, Tobias Preu√ü, Christian Rijke",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/teikei/teikei/issues"
  },
  "homepage": "https://github.com/teikei/teikei",
  "dependencies": {
    "backbone": "^1.2.3",
    "backbone-forms": "^0.14.0",
    "backbone.marionette": "^1.8.5",
    "blueimp-file-upload": "^9.11.2",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.0",
    "leaflet": "^0.7.3",
    "leaflet.markercluster": "^0.5.0",
    "normalize-css": "^2.3.1",
    "react": "^15.3.0",
    "react-autocomplete": "^1.1.0",
    "react-dom": "^15.3.0",
    "react-modal": "^1.4.0",
    "select2": "^3.4.8",
    "spin": "^0.0.1",
    "superagent": "^2.1.0",
    "timeago": "^0.2.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "asciify": "^1.3.5",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "express": "^4.14.0",
    "express-http-proxy": "^0.9.1",
    "http-proxy-middleware": "^0.17.0",
    "node-env-file": "^0.1.7",
    "nwb": "^0.11.1",
    "path": "^0.12.7"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "comma-dangle": [
        2,
        "never"
      ]
    },
    "globals": {
      "Backbone": true,
      "Marionette": true,
      "Entities": true,
      "Base": true,
      "I18n": true,
      "Alert": true,
      "Places": true,
      "MarkerIcon": true,
      "PlacesList": true,
      "L": true,
      "JST": true,
      "_": true,
      "Teikei": true,
      "User": true,
      "$": true,
      "editors": true
    }
  }
}
